{
  "version": "0.1.1",
  "name": "@ashigirl96/happy-notion",
  "type": "module",
  "bin": {
    "happy-notion": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["package.json", "dist"],
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && bun build-cli.ts && tsc -p tsconfig.json",
    "dev": "bun run src/cli.ts generate -c example/notion.config.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@notionhq/client": "2.2.15",
    "@types/bun": "latest",
    "bun-plugin-isolated-decl": "0.1.6",
    "oxc-transform": "0.44.0"
  },
  "peerDependencies": {
    "typescript": "5.7.2"
  },
  "dependencies": {
    "commander": "12.1.0",
    "ts-morph": "24.0.0",
    "zod": "3.24.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
